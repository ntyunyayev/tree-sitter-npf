{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "npf",
  "rules": {
    "document": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "section"
      }
    },
    "section": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "bash_section"
        },
        {
          "type": "SYMBOL",
          "name": "python_section"
        },
        {
          "type": "SYMBOL",
          "name": "txt_section"
        }
      ]
    },
    "bash_section": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "bash_section_header"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "raw_content"
          },
          "named": true,
          "value": "bash_content"
        }
      ]
    },
    "txt_section": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "txt_section_header"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "raw_content"
          },
          "named": true,
          "value": "txt_content"
        }
      ]
    },
    "python_section": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "python_section_header"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "raw_content"
          },
          "named": true,
          "value": "python_content"
        }
      ]
    },
    "tag": {
      "type": "SYMBOL",
      "name": "alpha_numeric"
    },
    "tags": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "tag"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "STRING",
                    "value": ","
                  },
                  {
                    "type": "STRING",
                    "value": "|"
                  }
                ]
              },
              {
                "type": "SYMBOL",
                "name": "tag"
              }
            ]
          }
        }
      ]
    },
    "role": {
      "type": "PATTERN",
      "value": "[^\\S\\n]+"
    },
    "options": {
      "type": "PATTERN",
      "value": "[^\\@\\n]+"
    },
    "bash_section_names": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "%script"
        }
      ]
    },
    "python_section_names": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "%pyexit"
        },
        {
          "type": "STRING",
          "value": "%variables"
        },
        {
          "type": "STRING",
          "value": "%late_variables"
        },
        {
          "type": "STRING",
          "value": "%config"
        }
      ]
    },
    "txt_section_names": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "%info"
        }
      ]
    },
    "bash_section_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "tags"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "bash_section_names"
          },
          "named": true,
          "value": "section_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "role"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "options"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "txt_section_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "tags"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "txt_section_names"
          },
          "named": true,
          "value": "section_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "role"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "options"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "python_section_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "tags"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "python_section_names"
          },
          "named": true,
          "value": "section_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "role"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "options"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "raw_content": {
      "type": "REPEAT1",
      "content": {
        "type": "PATTERN",
        "value": "[^%\\n]*\\n"
      }
    },
    "alpha_numeric": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_-]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
